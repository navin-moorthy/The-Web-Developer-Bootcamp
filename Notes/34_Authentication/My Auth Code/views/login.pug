extends base

block vars
 - var title = "Login"
 
block body
 h1 Login to your Account
 
 if error
  p ERROR: #{error}
  
 form(method="post")
  span Email:
  input(type="text", name="email", required= true)
  br
  
  span Password:
  input(type="password", name="password", required= true)
  br
  
  input(type="submit")